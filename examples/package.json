{
  "name": "isomorphic-counter",
  "version": "1.0.0",
  "description": "",
  "main": "routes.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Francisco Ryan Tolmasky I",
  "license": "MIT",
  "engines": {
    "node": "8.x.x"
  },
  "isomorphic": {
    "transforms": [
      {
        "match": "**/*.js",
        "transform": "isomorphic-javascript",
        "options": {
          "babel": {
            "presets": [
              [
                "isomorphic-preset",
                {
                  "node": "8.x.x",
                  "react": true
                }
              ]
            ]
          }
        }
      }
    ],
    "public": {
      "/assets/*": ":asset-domain/assets/:name.bundle.js"
    },
    "entrypoints": {
      "/(*_/):name.js": {
        "output": "/assets/:name.bundle.js",
        "transform": "isomorphic-bundle-js",
        "options": {
          "minify": true,
          "babel": {
            "parserOpts": {
              "allowReturnOutsideFunction": true
            },
            "presets": [
              [
                "isomorphic-preset",
                {
                  "browser": true,
                  "react": true
                }
              ]
            ]
          }
        }
      },
      "/(*_/):name.less": {
        "output": "/assets/styles/:name.css",
        "transform": "isomorphic-less"
      },
      "/(*_/):name.sass": {
        "output": "/assets/styles/:name.css",
        "transform": "isomorphic-sass"
      },
      "/(*_/):name.scss": {
        "output": "/assets/styles/:name.css",
        "transform": "isomorphic-sass"
      }
    }
  },
  "dependencies": {
    "express": "^4.16.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "uuid": "^3.1.0"
  }
}
